{
  "name": "egg-vue-webpack-boilerplate",
  "version": "0.0.1",
  "description": "基于egg-vue-webpack-dev和egg-view-vue(ssr)插件的工程骨架项目",
  "scripts": {
    "build": "BUILD_ENV=prod node build/build.js",
    "build-dev": "BUILD_ENV=dev node build/build.js",
    "build-test": "BUILD_ENV=test node build/build.js",
    "build-prod": "BUILD_ENV=prod node build/build.js",
    "kill-port": "kill -9 $(lsof -i:8888 |grep 'node'|awk '{print $2}') ",
    "start": "WORKERS=1 node index.js",
    "start-test": "WORKERS=1 npm run build-test && NODE_ENV=prodcution node index.js",
    "start-prod": "WORKERS=1 npm run build-prod && NODE_ENV=prodcution node index.js",
    "restart": "npm run kill-port && npm run start",
    "lint": "eslint .",
    "fix": "eslint --fix ."
  },
  "dependencies": {
    "@hubcarl/egg-view-vue": "^0.0.1",
    "axios": "^0.15.3",
    "co": "^4.6.0",
    "co-wait": "^0.0.0",
    "core-js": "^2.4.0",
    "egg": "^1.0.0-rc.3",
    "egg-logger": "^1.5.0",
    "egg-logview": "^1.0.0",
    "egg-validate": "^1.0.0",
    "egg-view-vue-ssr": "^0.0.1",
    "element-ui": "^1.2.5",
    "extend": "~3.0.0",
    "lodash": "^4.17.4",
    "lru-cache": "^4.0.1",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.17.1",
    "serialize-javascript": "^1.3.0",
    "vue": "^2.2.4",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.1.1",
    "vue-server-renderer": "^2.1.10",
    "vuex": "^2.1.1",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "autod-egg": "^1.0.0",
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-runtime": "^6.9.2",
    "css-loader": "^0.27.3",
    "egg-vue-webpack-dev": "^0.0.2",
    "eslint": "^3.18.0",
    "eslint-config-egg": "^3.2.0",
    "eslint-plugin-vue": "^2.0.1",
    "file-loader": "^0.10.1",
    "less": "^2.7.2",
    "less-loader": "^3.0.0",
    "node-noop": "^1.0.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^6.0.3",
    "url-loader": "^0.5.8",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.4",
    "webpack": "^2.2.0",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-merge": "^2.4.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "ci": {
    "version": "6, 7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubcarl/egg-vue-webpack-boilerplate.git"
  },
  "author": "hubcarl@126.com",
  "license": "MIT",
  "homepage": "https://github.com/hubcarl/egg-vue-webpack-boilerplate"
}
